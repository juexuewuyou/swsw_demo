     let reg = /^\\d{6}$/;\r\n                let reupwd = this.reupwd;\r\n                if(reg.test(reupwd) && this.reupwd==this.upwd){\r\n                     this.reupwdState='success';\r\n                     return true;\r\n                }else{\r\n                    this.reupwdState='error';\r\n                    return false;\r\n                }\r\n            },\r\n            // 验证表单,点击按钮后触发\r\n            checkForm(){\r\n                 if(this.checkUsername() && this.checkUpwd() && this.checkReupwd()){  // 所有组件验证成功\r\n                    let url='/register';\r\n                    let param=`username=${this.username}&password=${this.pwd}`;\r\n                    this.axios.post(url,param).then(result=>{\r\n                        console.log(result);\r\n                        if(result.data.code==200){\r\n                            this.$toast({\r\n                                message:'注册成功',\r\n                                position:'bottom',\r\n                                duration:3000\r\n                            })\r\n                            this.$router.push('login');\r\n                        }else{\r\n                            this.$messagebox('提示信息','用户已存在');\r\n                        }\r\n                    })\r\n                 }else{ // 其中有格式错误的组件\r\n\r\n                 }\r\n            }\r\n        }\r\n    }\r\n</script>\r\n<style scoped>\r\n    .logo img{\r\n        display: block;\r\n        width: 200px;\r\n        height: 200px;\r\n        margin: 0 auto;\r\n    }\r\n    .btn{\r\n    display: block;\r\n    margin: 0 auto;\r\n    margin-top: 10px;\r\n    width: 260px;\r\n    background-color:#afdfe4;\r\n    border-radius: 30px;\r\n    box-shadow: 3px 0px 5px 1px gray;\r\n}\r\n</style>"]}]}