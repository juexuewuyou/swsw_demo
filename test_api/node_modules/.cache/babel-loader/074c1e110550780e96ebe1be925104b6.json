AAA,KAAA,MAAA;;AACA,UAAA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,KAAA,MAAA,IAAA,KAAA,IAAA,EAAA;AACA,aAAA,WAAA,GAAA,SAAA;AACA,eAAA,IAAA;AACA,OAHA,MAGA;AACA,aAAA,WAAA,GAAA,OAAA;AACA,eAAA,KAAA;AACA;AACA,KApCA;AAqCA;AACA,IAAA,SAtCA,uBAsCA;AAAA;;AACA,UAAA,KAAA,aAAA,MAAA,KAAA,SAAA,EAAA,IAAA,KAAA,WAAA,EAAA,EAAA;AAAA;AACA,YAAA,GAAA,GAAA,WAAA;AACA,YAAA,KAAA,sBAAA,KAAA,QAAA,uBAAA,KAAA,GAAA,CAAA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,GAAA,EAAA,KAAA,EAAA,IAAA,CAAA,UAAA,MAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,MAAA;;AACA,cAAA,MAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,YAAA,KAAA,CAAA,MAAA,CAAA;AACA,cAAA,OAAA,EAAA,MADA;AAEA,cAAA,QAAA,EAAA,QAFA;AAGA,cAAA,QAAA,EAAA;AAHA,aAAA;;AAKA,YAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA,WAPA,MAOA;AACA,YAAA,KAAA,CAAA,WAAA,CAAA,MAAA,EAAA,OAAA;AACA;AACA,SAZA;AAaA,OAhBA,MAgBA,CAAA;AAEA;AACA;AA1DA;AAXA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <!-- <mt-header class=\"hr\" title=\"\">\r\n            <mt-button class=\"btn\" icon=\"back\" slot=\"left\"></mt-button>\r\n            <router-link class=\"rk\" to=\"/login\" slot=\"right\">登录</router-link>\r\n        </mt-header> -->\r\n        <div class=\"logo\">\r\n            <img src=\"../assets/login/logo.png\" alt=\"\">\r\n        </div>\r\n        <mt-field \r\n        label=\"用户名\" \r\n        placeholder=\"请输入用户名\" \r\n        type=\"text\"\r\n        v-model=\"username\"\r\n        :state=\"usernameState\"\r\n        @blur.native.capture=\"checkUsername\"></mt-field>\r\n        <mt-field\r\n        label=\"邮箱\" \r\n        type=\"text\"\r\n        placeholder=\"请输入邮箱\">\r\n           \r\n        </mt-field>\r\n        <mt-field \r\n        label=\"密码\" \r\n        placeholder=\"请输入密码\" \r\n        type=\"password\"\r\n        v-model=\"upwd\"\r\n        :state=\"upwdState\"\r\n         @blur.native.capture=\"checkUpwd\"></mt-field>\r\n        <mt-field \r\n         label=\"确认密码\"\r\n         placeholder=\"请再次输入密码\" \r\n         type=\"password\"\r\n         v-model=\"reupwd\"\r\n         :state=\"reupwdState\"\r\n         @blur.native.capture=\"checkReupwd\"></mt-field>\r\n         <mt-button @click=\"checkForm\" class=\"btn\">免费注册</mt-button>\r\n    </div>\r\n</template>\r\n<script>\r\n    export default{\r\n        data(){\r\n            return{\r\n                username:'',\r\n                usernameState:'',\r\n                upwd:'',\r\n                upwdState:'',\r\n                reupwd:'',\r\n                reupwdState:'',\r\n            } \r\n        },\r\n        methods:{\r\n            //用户名验证\r\n            checkUsername(){\r\n                let username = this.username;\r\n                let reg = /^\\w{6,12}$/;\r\n                if(reg.test(username)){\r\n                    this.usernameState='success';\r\n                    return true;\r\n                }else{\r\n                    this.usernameState='error';\r\n                    return false;\r\n                }\r\n            },\r\n            //密码验证\r\n            checkUpwd(){\r\n                let upwd = this.upwd;\r\n                let reg = /^\\d{6}$/;\r\n                if(reg.test(upwd)){\r\n                    this.upwdState='success';\r\n                    return true;\r\n                }else{\r\n                    this.upwdState='error';\r\n                    return false;\r\n                }\r\n            },\r\n            //密码再次验证\r\n            checkReupwd(){\r\n                 let reg = /^\\d{6}$/;\r\n                let reupwd = this.reupwd;\r\n                if(reg.test(reupwd) && this.reupwd==this.upwd){\r\n                     this.reupwdState='success';\r\n                     return true;\r\n                }else{\r\n                    this.reupwdState='error';\r\n                    return false;\r\n                }\r\n            },\r\n            // 验证表单,点击按钮后触发\r\n            checkForm(){\r\n                 if(this.checkUsername() && this.checkUpwd() && this.checkReupwd()){  // 所有组件验证成功\r\n                    let url='/register';\r\n                    let param=`username=${this.username}&password=${this.pwd}`;\r\n                    this.axios.post(url,param).then(result=>{\r\n                        console.log(result);\r\n                        if(result.data.code==200){\r\n                            this.$toast({\r\n                                message:'注册成功',\r\n                                position:'bottom',\r\n                                duration:3000\r\n                            })\r\n                            this.$router.push('login');\r\n                        }else{\r\n                            this.$messagebox('提示信息','用户已存在');\r\n                        }\r\n                    })\r\n                 }else{ // 其中有格式错误的组件\r\n\r\n                 }\r\n            }\r\n        }\r\n    }\r\n</script>\r\n<style scoped>\r\n    .logo img{\r\n        display: block;\r\n        width: 200px;\r\n        height: 200px;\r\n        margin: 0 auto;\r\n    }\r\n    .btn{\r\n    display: block;\r\n    margin: 0 auto;\r\n    margin-top: 10px;\r\n    width: 260px;\r\n    background-color:#afdfe4;\r\n    border-radius: 30px;\r\n    box-shadow: 3px 0px 5px 1px gray;\r\n}\r\n</style>"],"sourceRoot":"src/tasting"}]}